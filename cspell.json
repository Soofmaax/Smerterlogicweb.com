  "version": "0.2",
  "language": "en,fr",
  "allowCompoundWords": true,
  "dictionaryDefinitions": [
    { "name": "en-us", "path": "node_modules/@cspell/dict-en-us/cspell-ext.json" },
    { "name": "fr-fr", "path": "node_modules/@cspell/dict-fr-fr/cspell-ext.json" },
    { "name": "software-terms", "path": "node_modules/@cspell/dict-software-terms/cspell-ext.json" }
  ],
  "dictionaries": [
    "en-us",
    "fr-fr",
